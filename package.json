{
  "name": "advent-of-code-2021",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "test": "jest",
    "lint": "eslint --fix",
    "format": "prettier --write",
    "typecheck": "tsc --noEmit --esModuleInterop --target es2015 --moduleResolution node",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^6.0.0",
    "jest": "^29.2.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "ts-jest": "^29.0.3",
    "typescript": "4.9.4"
  },
  "lint-staged": {
    "*.ts": [
      "pnpm run lint",
      "pnpm run format",
      "pnpm run typecheck"
    ]
  },
  "engines": {
    "node": "18"
  },
  "license": "ISC"
}
